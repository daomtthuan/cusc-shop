{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "experimentalDecorators": true,
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "typeRoots": ["./node_modules/@types", "./type"],
    "types": ["@types/node", "@nuxt/types", "@types/nuxtjs__auth", "bootstrap-vue", "@nuxtjs/axios", "@types/vuelidate", "@types/vue2-editor"]
  },
  "exclude": ["node_modules"]
}
